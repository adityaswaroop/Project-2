= form_for(@company) do |f|
  - if @company.errors.any?
    #error_explanation
      %h2
        = pluralize(@company.errors.count, "error")
        prohibited this company from being saved:
      %ul
        - @company.errors.full_messages.each do |msg|
          %li= msg
  %script{:src => "/assets/companies.js", :type => "text/javascript"}
    :javascript
      document.write('<style type="text/css">');
      document.write('div.domtab div{display:none;}<');
      document.write('/s'+'tyle>');
  %p#domtabprintview
  .domtab
    %ol.domtabs
      %li
        %a{:href => "#what"} What is DOMtab?
      %li
        %a{:href => "#how"} How to use DOMtab
      %li
        %a{:href => "#pr"} Previous and next links
      %li
        %a{:href => "#style"} How to style DOMtab
      %li
        %a{:href => "#dl"} Download
    %div
      %h2
        %a#what{:name => "what"} What is DOMtab?
    %div
      %h2
        %a#how{:name => "how"} How to use DOMtab

